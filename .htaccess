<IfModule mod_rewrite.c>
    RewriteEngine on

    # Redirect HTTP to HTTPS
    RewriteCond %{HTTPS} !=on
    RewriteRule ^ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

    # Rewrite requests to the Laravel public directory
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

